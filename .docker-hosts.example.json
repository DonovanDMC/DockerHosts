{
    "hostsFile": "/etc/hosts",
    "host": "localhost:2375",
    "socket": "/var/run/docker.sock",
    "suffix": ".containers.local",
    "stackSuffixes": {
        "stack1": ".stack1.containers.local"
    },
    "containerHosts": {
        "my-awsome-project": "awesome.containers.local",
        "keep.names.in.tact": "soething-else.containers.local"
    },
    "debug": false,
    "silent": false,
    "idHosts": false,
    "keepPeriods": false,
    "keepStacks": [
        "stack1",
        "my-cool-stuff"
    ]
}
